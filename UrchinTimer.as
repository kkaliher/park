class UrchinTimer{	private var startTime:Number;	private var increment:Number;	private var urchinString:String;	private var postEvery:Number;	private var classObj:Object;	private var timerInterval:Number;		function UrchinTimer(target:MovieClip){		trace ("created UrchinTimer");		classObj = this;		urchinString = "/tenSeconds";		postEvery = 15 //seconds		timerInterval = setInterval(classObj,"postTime",postEvery*1000);		trace ("timerInterval = "+timerInterval);	}		private function postTime(){		trace ("posting "+this.postEvery+" seconds using urchinTracker");		getURL("javascript:urchinTracker('"+this.urchinString+"');");	}	public function destroy(){		trace ("destroy UrchinTimer");		trace ("clear timerInterval "+timerInterval);		clearInterval(timerInterval);		delete classObj;	}}		