class Jogger extends MovieClip{	//declare variables	private var self:MovieClip;	private var pace:Number;	private var flip:Number;		//constructor	function Jogger(){		trace ("new Jogger");		self = this;		if (flip==undefined){			flip=1;		}		_xscale=flip*7;		_yscale=7;		_x=_parent.w/2-flip*(_parent.w/2+5);		_y=2*_parent.h/3;		_alpha=80;		pace = .7;		onEnterFrame = function(){			jog();		}	}		private function jog(){		_x+=pace*flip;		_y=_parent.G.getGroundY(Math.floor(_x));		if (_x-7>_parent.w or _x<-7){			destroy();		}	}		public function destroy(){		trace ("destroy jogger");		delete onEnterFrame;		var f=flip;		_parent.classObj.joggerInterval = setInterval(_parent.classObj,"make",10000+random(10)*1000,"jogger",1000,{flip:-f});		self.removeMovieClip();	}}