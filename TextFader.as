class TextFader {		var parentMC:MovieClip;	var textFader:MovieClip;	var rt:MovieClip;	var classObj:Object;	var textAlpha:Number;	var subject:String;		function TextFader(pMC:MovieClip) {		parentMC = pMC;		rt=parentMC._parent;		trace("new TextFader");		rt.mailButton._y = 5.5+(2*Stage.height/3);		rt.bannerText._y = 5+(2*Stage.height/3);		rt.bannerText.text = "grow";		rt.bannerText._alpha = 0;		textFader = parentMC.createEmptyMovieClip("textFader", -600);		classObj = this;		textFader.classObj = classObj;		textFader.onEnterFrame = function() {			this.classObj.checkGround();		}	}		function checkGround():Void {		var X = parentMC.G.getX();		if (X<51) {			textAlpha = 2*(X);			rt.bannerText._alpha = textAlpha;		}		if (X>200 and X<251) {			textAlpha = 2*(250-X);			if (textAlpha<=2) {				textAlpha = 0;				rt.bannerText.text = "fairpark";			}			rt.bannerText._alpha = textAlpha;		}		if (X>parentMC.w-50) {			rt.bannerText._alpha = 2*(X-(parentMC.w-50));			rt.mailButton.onPress = function() {				getURL("mailto:info@fairparkstudio.com?subject=hi");				getURL("javascript:urchinTracker('/emailCreated.html');");			};		}		if (X>=parentMC.w) {			trace("end");			rt.doEnd();			delete textFader.onEnterFrame;			//doReset();		}	}		public function destroy(){		delete textFader.onEnterFrame;		parentMC.textFader.removeMovieClip();		delete classObj;	}}